<?xml version='1.0' encoding='utf-8'?>
<widget id="com.bite.cordova" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Accelerometer">
        <param name="android-package" value="org.apache.cordova.devicemotion.AccelListener" />
    </feature>
    <feature name="Compass">
        <param name="android-package" value="org.apache.cordova.deviceorientation.CompassListener" />
    </feature>
    <name>BITE</name>
    <description>
        A local food-listing aggregator.
    </description>
    <author email="samheutmaker@gmail.com" href="http://www.samheutmaker.com">
        Sam Heutmaker
    </author>
    <content src="index.html" />
    <access origin="*" />
</widget>
